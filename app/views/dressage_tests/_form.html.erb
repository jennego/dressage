<%= simple_form_for @dressage_test do |f| %>
  <%= f.input :org_name %>
  <%= f.input :name %>
  <%= f.input :level %>
  <%= f.input :year %>

<p> Enter test </p>


<%= f.fields_for :moves do |f| %>
    <%= render 'move_fields', :f => f %>
     <% end %>

<div class='links'>
  <%= link_to_add_association 'add move', f, :moves %>
</div>



<%= f.submit class: 'btn btn-primary' %>

<% end %>
